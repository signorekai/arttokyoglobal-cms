{
  "kind": "collectionType",
  "collectionName": "collections",
  "info": {
    "singularName": "collection",
    "pluralName": "collections",
    "displayName": "Collection",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "title": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "CurrentlyMinting",
        "MintingPaused",
        "FinishedMinting"
      ]
    },
    "mintPrice": {
      "type": "float"
    },
    "artist": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::artist.artist",
      "inversedBy": "collections"
    },
    "coverImg": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "contractAddress": {
      "type": "string",
      "unique": true
    },
    "tokens": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::token.token",
      "mappedBy": "collection"
    },
    "description": {
      "type": "customField",
      "options": {
        "preset": "standard"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "totalTokens": {
      "type": "integer",
      "required": true
    },
    "ABI": {
      "type": "json"
    },
    "CID": {
      "type": "string"
    },
    "totalSupply": {
      "type": "integer"
    },
    "whitelistMintEnabled": {
      "type": "boolean"
    },
    "dynamicStart": {
      "type": "boolean"
    },
    "startDate": {
      "type": "integer"
    }
  }
}
